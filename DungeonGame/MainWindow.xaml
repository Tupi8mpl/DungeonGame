<Window x:Class="DungeonGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DungeonGame"
        mc:Ignorable="d"
        Title="Dungeon Game WPF" Height="720" Width="640">
    <Canvas Name = "MyCanvas" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp" Background="Gray">
        <Image Height="40" Width="50" Source="images/chest.png" Canvas.Left="0" Canvas.Top = "0" Tag="chest" /> 
    </Canvas>
</Window>
